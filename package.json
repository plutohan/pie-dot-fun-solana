{
  "name": "@ao-labs/pie-dot-fun-solana",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/ao-labs/pie-dot-fun-solana"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "sdk/index.ts",
  "author": "ao-labs",
  "license": "ISC",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "test-devnet": "yarn run ts-mocha -p ./tsconfig.json -t 1000000 tests/pie_devnet.ts",
    "test-cpmm-devnet": "yarn run ts-mocha -p ./tsconfig.json -t 1000000 tests/pie_devnet_cpmm.ts",
    "test-mainnet": "yarn run ts-mocha -p ./tsconfig.json -t 1000000 tests/pie_mainnet.ts",
    "test-server": "yarn run ts-mocha -p ./tsconfig.json -t 1000000 tests/pie_server.ts",
    "deploy-devnet": "anchor deploy --program-name pie --provider.cluster https://api.devnet.solana.com",
    "deploy-mainnet": "anchor deploy --program-name pie --provider.cluster mainnet --provider.wallet .local/mainnet/admin.json -- --max-sign-attempts 100 --with-compute-unit-price 1000000",
    "rebuild": "rm -rf target && anchor keys sync && anchor build",
    "rebuild-devnet": "rm -rf target && anchor keys sync && anchor build -- --features devnet"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@raydium-io/raydium-sdk-v2": "0.1.79-alpha",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.95.5",
    "axios": "^1.7.9",
    "bs58": "^6.0.0",
    "console-table-printer": "^2.12.1",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
